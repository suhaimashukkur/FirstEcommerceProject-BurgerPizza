<script>
// Function to update the cart
// Function to update the cart
// Function to update the subtotal and cart total
function updateCart(productId, quantity,totalPrice) {
  fetch('/updateCart', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify({
      productId: productId,
      quantity: quantity,
      totalPrice:totalPrice
    })
  })
  .then(response => response.json())
  .then(response => {
    let totalPrice = 0;
    if (response && response.success) {
      // Update the individual subtotal for the item
      const subtotalElement = document.getElementById(`subtotalId_${productId}`);
      if (subtotalElement) {
        subtotalElement.textContent = response.newSubtotal;
      }
      
      // Update the total price in the UI
      const totalPriceElement = document.getElementById("totalprice");
      if (totalPriceElement) {
        totalPriceElement.textContent = response.totalPrice;
      }

      // Update the subtotal in the UI
      




      
      const subtotal = document.getElementById("subtotal");
      if (subtotal) {
        subtotal.textContent = response.totalPrice; // Update the total price
      }

      // Show the total price container if hidden
      const totalPriceContainer = document.getElementById("totalPriceContainer");
      if (totalPriceContainer) {
        totalPriceContainer.style.display = "block";
      }
    } else {
      console.error('Failed to update cart.');
    }
  })
  .catch(error => {
    console.error('Error updating cart: ', error);
  });
}

// Event listeners for quantity change buttons
$(document).ready(function () {
  $('.increase').click(function () {
    var inputElement = $(this).closest('.quantity-container').find('.quantity-amount');
    var productId = inputElement.data('product-id');
    var currentQuantity = parseInt(inputElement.val());
    var newQuantity = currentQuantity + 1;
    var totalPriceElement = document.getElementById('totalPrice')

    
    var totalPrice = totalPriceElement.textContent
    inputElement.val(newQuantity);
    updateCart(productId,newQuantity,totalPrice)
  });

  $('.decrease').click(function () {
    var inputElement = $(this).closest('.quantity-container').find('.quantity-amount');
    var productId = inputElement.data('product-id');
    var currentQuantity = parseInt(inputElement.val());

    if (currentQuantity > 1) {
      var newQuantity = currentQuantity - 1;
      var totalPriceElement = document.getElementById('totalPrice')
      var totalPrice = totalPriceElement.textContent
      console.log('Decrease button clicked. ProductId:', productId, 'New Quantity:', newQuantity, totalPrice);

      inputElement.val(newQuantity);
      updateCart(productId, newQuantity,totalPrice);
       
    }
  });
});

</script>

