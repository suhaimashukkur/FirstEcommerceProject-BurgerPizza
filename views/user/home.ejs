      <div class="container">
        <div class="row">
          
          <!-- Sorting and Filtering Options in a Card -->
          <div class="col-lg-2">
            <div class="card border">
              <div class="card-body">
                
              <div>
                <div class="sort-options">
                  <label for="sortOptions" style="font-weight:500;"><spam class="text-danger">Sort</spam> <spam> </spam>Products</label>
                </div>
                  <select id="sortOptions" onchange="sortProducts()">
                    <option value="name">Name</option>
                    <option value="price">Price</option>
                    <!-- Add more options as needed -->
                  </select>
                  <label for="categoryFilter" style="font-weight: 500;"><spam class="text-danger">Category</spam> <spam> </spam>Filter</label>
                  <select id="categoryFilter" onchange="filterProducts()">
                    <option value="">All</option>
                    <!-- Populate options dynamically from backend -->
                    <% category.forEach(cat => { %>
                      <option value="<%= cat._id %>"><%= cat.categoryName %></option>
                    <% }) %>
                  </select>
                </div>
              </div>
            </div>
          </div>
    
          <!-- Product Display in Another Card -->
          <div class="col-lg-10">
            <div class="card border">
              <div class="card-body">
               
    
                <!-- Product Display -->
                <div class="row border-left pl-4" id="search-results">
                  <!-- Product display goes here -->
                  <% product.forEach(product => { %>
                    <div class="col-lg-3 col-md-4 col-sm-6 g-4 pl-4 ">
                      <div class="card mb-4 h-100 ">
                        <img src="/uploads/<%= product.image1 %>" class="card-img-top img-fluid" alt="Product Image" style="height: 200px; object-fit: cover;">
                        <div class="card-body ">
                          <ul class="list-unstyled d-flex justify-content-between align-items-center">
                            <li>
                              <a href="/addToCart/<%= product._id %>"><i class="fa fa-heart text-danger"></i></a>
                            </li>
                            <li>
                              <a href="#"><i class="fa fa-retweet text-danger"></i></a>
                            </li>
                            <li>
                              <a href="#"><i class="fa fa-shopping-cart text-danger"></i></a>
                            </li>
                          </ul>
                          <h5 class="card-title">
                            <a href="/single-product/<%= product._id %>" style="color: #dd1d1d;justify-content: center;font-weight: 700;"><%= product.name %></a>
                          </h5>
                          <h6 style="font-weight: 700;">Rs: <%= product.price %>/-</h6>
                        </div>
                      </div>
                    </div>
                  <% }) %>
                  
                </div>
                <br><br>
    
                <!-- Pagination -->
                <div class="row">
                  <div class="col-lg-12">
                    <nav class="pagination-container">
                      <button class="pagination-button btn btn-danger" id="prev-button" title="Previous page" aria-label="Previous page">
                        &lt;
                      </button>
                      <div id="pagination-numbers"></div>
                      <button class="pagination-button btn btn-danger" id="next-button" title="Next page" aria-label="Next page">
                        &gt;
                      </button>
                    </nav>
                    <style>
                      .pagination-container {
                        display: flex;
                        justify-content: space-between;
                        align-items: center;
                      }
                      .pagination-button {
                        padding: 10px;
                        /* Add your button styles here */
                      }
                    </style>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    





    




    
   
