<script>
function filterProducts() {
  const productList = document.getElementById('search-results');
  const filterOption = document.getElementById('categoryFilter').value;

  fetch(`/product-filter?category=${filterOption}`, {
    method: 'GET',
    headers: { 'Content-Type': 'application/json' }
  })
    .then(res => res.json())
    .then(data => {
      productList.innerHTML = ''; // Clear existing content

      data.payload.forEach(product => {
        const productCard = document.createElement('div');
        productCard.classList.add('col-lg-3', 'col-md-4', 'col-sm-6', 'g-4');

        productCard.innerHTML = `
          <div class="card mb-4 h-100">
            <img src="/uploads/${product.image1}" class="card-img-top img-fluid" alt="Product Image" style="height: 200px; object-fit: cover;">
            <div class="card-body">
              <h5 class="card-title">
                <a href="/single-product/${product._id}" style="color: #dd1d1d; justify-content: center; font-weight: 700;">${product.name}</a>
              </h5>
              <h6 style="font-weight: 700;">Price: Rs. ${product.price}/-</h6>
            </div>
          </div>
        `;

        productList.appendChild(productCard);
      });
    })
    .catch(error => console.error('Error:', error));
}


</script>


        <script>
        
  function sortProducts() {
    const productList = document.getElementById('search-results');
    const sortOption = document.getElementById('sortOptions').value;

    fetch(`/product-sort?sort=${sortOption}`, {
      method: 'GET',
      headers: { 'Content-Type': 'application/json' }
    })
      .then(res => res.json())
      .then(data => {
        productList.innerHTML = '';

        data.payload.forEach(product => {
          const productCard = document.createElement('div');
          productCard.classList.add('col-lg-3', 'col-md-4', 'col-sm-6', 'g-4');
          productCard.innerHTML = `
            <div class="card mb-4 h-100">
              <img src="/uploads/${product.image1}" class="card-img-top img-fluid" alt="Product Image" style="height: 200px; object-fit: cover;">
              <div class="card-body">
                <h5 class="card-title">
                  <a href="/single-product/${product._id}" style="color: #dd1d1d; justify-content: center; font-weight: 700;">${product.name}</a>
                </h5>
                <h6 style="font-weight: 700;">Price: Rs. ${product.price}/-</h6>
              </div>
            </div>
          `;
          productList.appendChild(productCard);
        });
      })
      .catch(error => console.error('Error:', error));
  }
</script>

        


        
       
        <script>
        $(document).ready(function () {
          let currentPage = 1;
          const pageSize = 4; // Set your desired page size
        
          function fetchProducts(page) {
            $.ajax({
              method: 'post',
              url: '/pagination',
              data: {
                page: page,
                pageSize: pageSize
              },
              success: function (res) {
          const productList = document.getElementById('search-results');

          if (res && res.products.length > 0) {
            let productHTML = '';

            res.products.forEach((item) => {
              // Create a product card similar to the homepage
              productHTML += `
                <div class="col-lg-3 col-md-4 col-sm-6 g-4 pl-4">
                  <div class="card mb-4 h-100">
                    <img src="/uploads/${item.image1}" class="card-img-top img-fluid" alt="Product Image" style="height: 200px; object-fit: cover;">
                    <div class="card-body">
                      <!-- Add other product details -->
                      <h5 class="card-title">
                        <a href="/single-product/${item._id}" style="color: #dd1d1d; justify-content: center; font-weight: 700;">${item.name}</a>
                      </h5>
                      <h6 style="font-weight: 700;"> Rs. ${item.price}/-</h6>
                      <a href="/cart" class="btn btn-link" style="float:right;color:red">
                        <i class="fa fa-cart-plus"></i>
                      </a>
                    </div>
                  </div>
                </div>
              `;
            });

            productList.innerHTML = productHTML;
          }
        },
        error: function (error) {
          console.log(error);
        }
      });
    }

    fetchProducts(currentPage);

          // Pagination button click event handlers
          $('#prev-button').on('click', function () {
            if (currentPage > 1) {
              currentPage--;
              fetchProducts(currentPage);
            }
          });
        
          $('#next-button').on('click', function () {
            currentPage++;
            fetchProducts(currentPage);
        });
        });
        </script>
        
